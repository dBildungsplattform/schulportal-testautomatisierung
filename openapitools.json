{
	"$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
	"spaces": 2,
	"generator-cli": {
		"version": "6.6.0",
      "storageDir": "./base/api/storageDir",
      "generators": {
        "backend": {
          "generatorName": "typescript-fetch",
          "output": "#{cwd}/base/api/generated",
          "inputSpec": "#{cwd}/base/api/openapispec.json",
          "additionalProperties": {
            "supportsES6": false,
            "npmVersion": "9.5.1",
            "withInterfaces": true
          }
        }
      }
	}
}
